{
  "name": "renamer",
  "description": "Rename files in bulk",
  "version": "2.0.0",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "bin": "bin/cli.mjs",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "repository": "https://github.com/75lb/renamer",
  "files": [
    "index.mjs",
    "bin",
    "lib"
  ],
  "keywords": [
    "rename",
    "move",
    "mv",
    "file",
    "files",
    "bulk",
    "batch"
  ],
  "scripts": {
    "test1": "test-runner test/*.mjs test/internals/*.mjs",
    "test": "esm-runner test/*.mjs",
    "docs": "jsdoc2md index.mjs > docs/API.md",
    "cover": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "array-back": "^4.0.1",
    "chalk": "^4.0.0",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "fast-diff": "^1.2.0",
    "glob": "^7.1.6",
    "global-modules": "^2.0.0",
    "load-module": "^3.0.0",
    "node-version-matches": "^2.0.1",
    "printj": "^1.2.2",
    "reduce-flatten": "^3.0.0",
    "reduce-unique": "^2.0.1",
    "stream-read-all": "^3.0.0",
    "typical": "^6.0.1"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "esm-runner": "^0.3.4",
    "jsdoc-to-markdown": "^5.0.3",
    "mkdirp2": "^1.0.4",
    "nyc": "^15.0.1",
    "rimraf": "^3.0.2",
    "test-runner": "^0.8.13"
  }
}
